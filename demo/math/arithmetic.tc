import "io"

add := :(a: u16, b: u16) => a + b

mult := :(a: u16, b: u16) -> u16 {
  res := b
  while (a) {
    --a
    res = res + b
  }
  ret res
}

factorial := :(n: u16) -> u16 {
  a = 3
  b = :(n: u16) -> string {
    if n - 2 { ret "thing" } else { ret "other" }
  } (a)

  d: string
  if (a - 2) {
    d = "thing"
  } else {
    d = "other"
  }
}

square := :(n: u16) => mult(n, n)

area_sphere := :(r: u16) => mult(square(r), pi)

display_area_sphere := :(r: u16) {
  print(area_sphere(r))
}